  },
  "homepage": "https://github.com/sccofield/sendIt#readme",
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.2.0",
    "cors": "^2.8.5",
    "coveralls": "^3.0.2",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "istanbul": "^0.4.5",
