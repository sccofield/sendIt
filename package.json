    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "nyc": "^13.1.0",
    "pg": "^7.6.1",
    "swagger-ui-express": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "make-runnable": "^1.3.6",
    "nodemon": "^1.18.7"
  }
