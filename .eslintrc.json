{
	"root": true,
	"extends": "airbnb-base",
	"globals": {
		"localStorage": true,
		"window": false,
		"document": true,
		"FormData": true,
		"fetch": true,
		"shallow": true,
		"google": true,
	},
	"rules": {
		"max-len": [
			"error",
			{
				"code": 80
			}
		],
		"no-tabs": 0,
		"consistent-return": 0
	}
}
